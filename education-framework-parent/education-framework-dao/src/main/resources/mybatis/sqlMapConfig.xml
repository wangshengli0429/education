<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 定义别名 -->
    <typeAliases>
        <!-- 用户 -->
         
        <!-- 学生 -->
        <typeAlias type="com.education.framework.model.student.Student" alias="student"/>
        <typeAlias type="com.education.framework.model.student.vo.StudentVo" alias="studentVo"/>
		<!-- 教师 -->
        <typeAlias type="com.education.framework.model.teacher.vo.TeacherVo" alias="teacherVo"/>
        <typeAlias type="com.education.framework.model.teacher.Teacher" alias="teacher"/>
        <typeAlias type="com.education.framework.model.teacher.TeacherCertificate" alias="teacherCertificate"/>
        <typeAlias type="com.education.framework.model.teacher.TeacherCourse" alias="teacherCourse"/>
        <typeAlias type="com.education.framework.model.teacher.TeacherTime" alias="teacherTime"/>
		<!-- 订单 -->
        <typeAlias type="com.education.framework.model.order.Order" alias="order"/>
		<!-- 留言 评论系统 -->
        <typeAlias type="com.education.framework.model.note.Note" alias="note"/>
		
    </typeAliases>
    <!-- 这里不需要再注册mybatis的SQL映射文件 ，因为在Spring的配置文件中配置sqlSessionFactory时注册了mapper的路径-->
</configuration>  